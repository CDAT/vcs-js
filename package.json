{
  "name": "vcs-js",
  "version": "0.1.0",
  "description": "Javascript interface for vcs.",
  "main": "dist/vcs.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "build:release": "npm run build -- -p",
    "build-continuous": "webpack --progress --colors --watch",
    "karma": "npm run waitforserver && karma start --single-run",
    "karma:serve": "npm run waitforserver && karma start --auto-watch",
    "karma:travis": "karma start --browsers ChromeHeadlessNoSandbox --single-run",
    "karma:travis:wait": "npm run waitforserver && karma start --browsers ChromeHeadlessNoSandbox --single-run",
    "server": "node ./config/scripts/test-server-runner.js",
    "test": "run-p --race 'server -- -d :0.0' karma",
    "test:travis": "run-p --race server karma:travis",
    "test:serve": "run-p --race 'server -- -d :0.0' karma:serve",
    "waitforserver": "node ./config/scripts/wait-for-rpc-server.js",
    "demo": "webpack-dev-server",
    "demo1": "webpack-dev-server"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-plugin-istanbul": "4.1.5",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "commander": "2.11.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-plugin-chai-expect": "1.1.1",
    "inject-loader": "3.0.1",
    "karma": "2.0.0",
    "karma-chai": "0.1.0",
    "karma-chai-as-promised": "0.1.2",
    "karma-chai-plugins": "0.9.0",
    "karma-coverage": "1.1.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-mocha": "1.3.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "2.0.9",
    "kw-web-suite": "5.0.1",
    "mocha": "5.0.0",
    "npm-run-all": "4.1.2",
    "resemblejs": "2.7.1",
    "shelljs": "0.7.8",
    "wait-on": "2.1.0",
    "webpack-dev-server": "2.11.1"
  },
  "dependencies": {
    "axios": "0.17.1",
    "b64-to-blob": "1.2.19",
    "css-element-queries": "1.0.1",
    "hammerjs": "2.0.8",
    "monologue.js": "0.3.5",
    "mout": "1.1.0",
    "ndarray": "1.0.18",
    "isndarray": "1.0.0",
    "ndarray-pack": "1.2.1",
    "ndarray-unpack": "1.0.0",
    "paraviewweb": "3.0.3",
    "plotly.js": "1.33.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UV-CDAT/vcs-js"
  },
  "bugs": {
    "url": "https://github.com/UV-CDAT/vcs-js/issues"
  }
}
